{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "exclude": ["commitlint.config.ts"],
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "auto-imports.d.ts", "components.d.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
